{
  "name": "Metric Conversions",
  "version": "0.1",
  "permissions": [
     "tabs", "http://*/*", "https://*/*"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.js", "content.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
